{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "no-bitwise": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-extend-native": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-caller": 2,
    "no-empty": 2,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-unused-expressions": 0,
    "dot-notation": 0
  }
}
